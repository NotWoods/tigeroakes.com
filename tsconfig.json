{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "types": ["astro/client", "vite/client", "vite-plugin-pwa/client"],
    "allowSyntheticDefaultImports": true,
    "importHelpers": true
  },
  "exclude": [
    "./src/components/shortcodes/playground-react/**/*.tsx",
    "./src/components/demos/fluent-ui-css-tricks/*.tsx",
    "./functions/**/*"
  ]
}
