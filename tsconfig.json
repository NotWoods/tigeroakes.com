{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "types": ["astro/client", "vite/client", "vite-plugin-pwa/client"],
    "allowSyntheticDefaultImports": true,
    "importHelpers": true
  },
  "exclude": [
    "./functions/**/*",
    "./netlify/edge-functions/**/*",
    "./dist/**/*"
  ]
}
