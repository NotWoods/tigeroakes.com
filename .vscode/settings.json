{
  "[html]": {
    "editor.formatOnSave": false
  },
  "[css]": {
    "editor.formatOnSave": false
  },
  "json.schemas": [
    {
      "fileMatch": ["/src/content/json-resume/*.json"],
      "url": "https://raw.githubusercontent.com/jsonresume/resume-schema/v1.0.0/schema.json"
    }
  ],
  "search.exclude": {
    "**/node_modules/**": true,
    "dist/**": true
  },
  "cSpell.enableFiletypes": ["mdx"],
  "cSpell.words": [
    "Astro",
    "Composables",
    "drawables",
    "FLIK",
    "Foulplay",
    "HeleOn",
    "JetBrains",
    "Jetpack",
    "MathML",
    "Oakes",
    "rehype",
    "shortcodes",
    "skippable",
    "webm"
  ],
  "cSpell.language": "en,en-GB"
}
