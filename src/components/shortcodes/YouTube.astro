---
export interface Props {
  id: string;
}

const { id } = Astro.props as Props;
---

<div class="w-full aspect-w-16 aspect-h-9">
  <iframe
    width="800"
    height="450"
    src={`https://www.youtube-nocookie.com/embed/${id}`}
    frameborder="0"
    allow="autoplay; encrypted-media; picture-in-picture"
    allowfullscreen
    class="youtube"
    loading="lazy"
  >
  </iframe>
</div>
