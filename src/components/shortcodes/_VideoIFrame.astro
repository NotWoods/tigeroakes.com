---
export interface Props {
  src: string;
  title?: string;
}

const { src, title } = Astro.props as Props;
---

<iframe
  width="800"
  height="450"
  {src}
  {title}
  frameborder="0"
  allow="fullscreen; autoplay; encrypted-media; picture-in-picture"
  allowfullscreen
  class="block aspect-video w-full"
  loading="lazy"
>
</iframe>
