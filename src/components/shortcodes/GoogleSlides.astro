---
export interface Props {
  src: string;
}

const { src } = Astro.props as Props;
---

<iframe
  {src}
  frameborder="0"
  width="800"
  height="486"
  allow="fullscreen; autoplay; encrypted-media; picture-in-picture"
  allowfullscreen
  allow="fullscreen"
  class="block w-full"
  loading="lazy"></iframe>
