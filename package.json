{
  "name": "tigeroakes.com",
  "version": "3.0.0",
  "description": "My portfolio website. Designed to work offline, load quickly, and showcase some of my favorite projects.",
  "repository": "https://github.com/NotWoods/tigeroakes.com.git",
  "author": "Tiger Oakes <tigeroakes@gmail.com>",
  "private": true,
  "dependencies": {
    "sw-precache": "^5.2.1"
  },
  "devDependencies": {
    "@types/sw-precache": "^5.2.0",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "sw": "sw-precache --root=public --config sw-precache-config.js",
    "format": "prettier --write **/*.{md,js,yml,yaml,scss} content/**/*.json --ignore-path .gitignore"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "proseWrap": "always",
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "printWidth": 120
        }
      }
    ]
  }
}
