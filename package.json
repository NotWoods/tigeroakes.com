{
  "name": "tigeroakes.com",
  "version": "3.0.0",
  "description": "My portfolio website. Designed to work offline, load quickly, and showcase some of my favorite projects.",
  "repository": "https://github.com/NotWoods/tigeroakes.com.git",
  "author": "Tiger Oakes <tigeroakes@gmail.com>",
  "private": true,
  "dependencies": {
    "cssnano": "^4.1.10",
    "postcss-cli": "^7.1.0",
    "postcss-preset-env": "^6.7.0",
    "sw-precache": "^5.2.1"
  },
  "devDependencies": {
    "@types/sw-precache": "^5.2.0",
    "postcss-color-functional-notation": "^3.0.2",
    "postcss-color-hex-alpha": "^5.0.3",
    "postcss-custom-media": "^7.0.8",
    "postcss-gap-properties": "^2.0.0",
    "postcss-inset": "^1.0.0",
    "postcss-place": "^4.0.1",
    "postcss-selector-matches": "^4.0.0",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "build": "hugo",
    "postbuild": "sw-precache --root=public --config sw-precache-config.js",
    "format": "prettier --write **/*.{md,js,yml,yaml,scss} content/**/*.json --ignore-path .gitignore",
    "highlight": "hugo gen chromastyles --style=dracula > ./assets/css/highlight.css"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "proseWrap": "never"
  }
}
